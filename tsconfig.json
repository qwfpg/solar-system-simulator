{
  "version": "2.0.2",
  "compilerOptions": {
    "lib": ["es5", "es2015.promise", "dom", "dom.iterable", "es2015", "es6", "es2016"],
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "outDir": "dist",
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}